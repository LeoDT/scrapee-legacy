  "scripts": {
    "dev": "concurrently --names 'renderer,main,chrome' 'yarn run dev-renderer' 'yarn run dev-main' 'yarn run dev-chrome'",
    "dev-browser": "concurrently --names 'renderer,chrome' 'yarn run dev-renderer' 'yarn run dev-chrome'",
    "dev-renderer": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.config.renderer.dev.babel.js",
    "dev-main": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=1 NODE_ENV=development electron --inspect=5858 -r ./scripts/BabelRegister ./src/desktop-app/main.ts",
    "dev-chrome": "cross-env NODE_ENV=development webpack --watch --config scripts/webpack.config.chrome.dev.babel.js",
    "dev-chrome-nativeMessage": "cross-env NODE_ENV=development webpack --watch --config scripts/webpack.config.chrome.nativeMessage.dev.babel.js",
    "graphql-playground": "cross-env NODE_ENV=development nodemon -e js,ts,jsx,tsx,graphql --watch src ./scripts/graphqlPlayground.js",
    "graphql-codegen": "graphql-codegen",
    "dev-graphql": "concurrently --names 'codegen,playground' 'yarn run graphql-codegen --watch' 'yarn run graphql-playground'",
    "cosmos": "cosmos",
    "update-i18n": "./scripts/downloadTranslations.js"
  },
